{
  "manifest_version": 3,
  "name": "ChatGPT Bookmarks",
  "version": "1.0",
  "description": "Add bookmarks functionality to ChatGPT web interface",
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["https://chatgpt.com/*"],
      "css": ["styles.css"],
      "js": ["content.js"]
    }
  ]
}
